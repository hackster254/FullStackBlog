{
    "version": 2,
    "builds": [
        {
            "src": "./reactfrontend/src/App.js",
            "use": "@vercel/node"
        },
        {
            "src": "./reactfrontend/build",
            "use": "@vercel/static"
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/nodejsbackend/server.js"
        }
    ],
    "routes": [
        {
            "src": "/.*",
            "dest": "/reactfrontend/src/App.js"
        }
    ]
}